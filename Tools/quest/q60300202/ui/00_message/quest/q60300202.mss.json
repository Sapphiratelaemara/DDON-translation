      "DispType" : 0,
      "GmdIndex" : 12,
      "JumpGroupSerial" : 0,
      "Message" : {
        "En" : "",
        "Jp" : "私にとって、ここは――\n混乱と後悔の中でひた走った迷宮みたいなものだ"
      },
      "MsgSerial" : 796713,
      "MsgType" : 0,
